    "def get_plural(word): \n",
    "    if word.endswith('y'):  \n",
    "        return word[:-1] + 'ies'  \n",
    "    elif word[-1] in 'sx' or word[-2:] in ['sh', 'ch']:  \n",
    "        return word + 'es'  \n",
    "    else:  \n",
    "        return word + 's'  \n",
    "        \n",
    "s = input(str) \n",
    "print(get_plural(s))\n",
    "\n"
   ]
  }
